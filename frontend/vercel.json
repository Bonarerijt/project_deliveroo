{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "env": {
    "REACT_APP_API_URL": "https://project-deliveroo.onrender.com"
  },
  "rewrites": [
    {
      "source": "/((?!assets/|favicon.ico|robots.txt|manifest.json).*)",
      "destination": "/index.html"
    }
  ]
}
